<html>
    <head>
        <title>登陆页</title>
        <script type="text/javascript" src="https://rescdn.qqmail.com/node/ww/wwopenmng/js/sso/wwLogin-1.0.0.js"></script>
    </head>
    <body>
        <p>登陆方式</p>
        <p>
            <a href="{{auth_url}}">授权链接</a>
        </p>
        <p>
            <div id="qr_code" />
        </p>

        <script>
            window.WwLogin({
                "id" : "qr_code",  
                "appid" : "{{appid}}",
                "agentid" : "{{agentid}}",
                "redirect_uri" :"{{redirect_uri}}",
                "state" : "{{state}}"
            });
        </script>

    </body>
</html>

